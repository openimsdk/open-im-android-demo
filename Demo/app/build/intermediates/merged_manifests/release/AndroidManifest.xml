<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.openim.android.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="32" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="io.openim.android.demo.DemoApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Demo"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="io.openim.android.demo.ui.search.SendVerifyActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.ui.search.PersonDetailActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.ui.search.SearchPersonActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.ui.search.AddFriendActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.ui.login.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.ui.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name="io.openim.android.demo.SplashActivity"
            android:exported="true"
            android:theme="@style/SplashTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="io.openim.android.demo.ui.main.MainActivity" />
        <activity
            android:name="io.openim.android.ouicontact.ui.ChatActivity"
            android:exported="false" />
    </application>

</manifest>